#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
     
    int N,sum = 0;
    cin>>N;
    int arr[N];
    for (int i=0; i<N;i++){
        cin >>arr[i];
    }
    for (int i=0; i<N;i++) {
        sum +=arr[i];
    }
    
    cout << sum;
    
    return 0;
}
